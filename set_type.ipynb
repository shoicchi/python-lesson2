   "source": [
    "# セット（集合型）重複を許さない\n",
    "\n",
    "a = set([5, 3, 8, 5])\n",
    "a = {5, 3, 8, 5}\n",
    "print(a)\n",
    "print(5 in a)\n",
    "a.add(2)# 2をセットに加える\n",
    "a.remove(3)# 3を削除する\n",
    "print(a)\n",
    "print(len(a))"
   ]
