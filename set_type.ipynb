   "source": [
    "# 集合演算\n",
    "\n",
    "a = {1, 3, 5, 8}\n",
    "b = {3, 5, 8, 9}\n",
    "print(a | b)# 和集合\n",
    "print(a & b)# 積集合\n",
    "print(a -b)# 差集合"
   ]
  }
